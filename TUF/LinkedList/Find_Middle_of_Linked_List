## https://takeuforward.org/plus/dsa/linked-list/faqs--medium/find-middle-of-linked-list


/*Definition for Singly Linked List
class ListNode {
    int val;
    ListNode next;

    ListNode() {
        val = 0;
        next = null;
    }

    ListNode(int data1) {
        val = data1;
        next = null;
    }

    ListNode(int data1, ListNode next1) {
        val = data1;
        next = next1;
    }
}
*/

class Solution {
    public ListNode middleOfLinkedList(ListNode head) {
        ListNode fNode = head;
        ListNode sNode = head;

        while(fNode != null && fNode.next != null){
            fNode = fNode.next.next;
            sNode = sNode.next;
        }

        return sNode;

    }
}
