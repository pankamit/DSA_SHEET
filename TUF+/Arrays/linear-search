## https://takeuforward.org/plus/dsa/arrays/fundamentals/linear-search

class Solution {
    public int linearSearch(int arr[], int tar) {
		//Your code goes here
        int n = arr.length;
        
        for(int i=0;i<n;i++){
            if(arr[i] == tar){
                return i;
            }
        }

        return -1;

    }
}
